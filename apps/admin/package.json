{
  "name": "@example/admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Admin UI for DynamoDB Client Example",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "seed": "tsx scripts/seed-data.ts",
    "clean": "rm -rf dist .vite",
    "clean-data": "tsx scripts/clean-data.ts"
  },
  "dependencies": {
    "@aws-amplify/auth": "^6.17.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@exabugs/dynamodb-client": "^0.4.1",
    "@example/api-types": "workspace:*",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "aws-amplify": "^6.15.8",
    "dayjs": "^1.11.19",
    "react": "^19.0.0",
    "react-admin": "^5.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.19.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.0.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^2.1.9"
  }
}
